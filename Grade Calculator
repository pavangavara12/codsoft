import java.util.*;
public class grade {
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    System.out.printf("Enter marks scored in java : ");
    int java = input.nextInt();
    System.out.printf("Enter marks scored in python : ");
    int python = input.nextInt();
    System.out.printf("Enter marks scored in dsa : ");
    int dsa = input.nextInt();

    int sum = java + python + dsa;
    double total = (sum / 3.0);
    System.out.printf("Your total marks are %d\n",sum);
    System.out.printf("Your average percentage is %f\n",total);

    if(total>=90 && total<=100){
      System.out.printf("Your grade is A");
    }
    else if(total>=80 && total<=89){
     System.out.printf("Your grade is B");
    }
    else if(total>=70 && total<=79){
     System.out.printf("Your grade is C");
    }
    else if(total>=60 && total<=69){
     System.out.printf("Your grade is D");
    }
    else if(total>=40 && total<=59){
     System.out.printf("Your grade is E");
    }
    else {
     System.out.printf("Your have failed");
    }
    input.close();
  }
}

